{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#8886e3b2a5841f75dc7070333fe77ace67191913",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "github:angular/common-builds#bf8eb32fefd0c27fff11e8bf97e58f2616bc8e09",
    "@angular/compiler": "github:angular/compiler-builds#753177a5c6b049cc207423a2f831f25be31a2966",
    "@angular/core": "github:angular/core-builds#b5cbd5970e1c10d620fc00af12d561eb9a3ed3fe",
    "@angular/forms": "github:angular/forms-builds#a24f9680f84e6843a58c27a703900df637a3a464",
    "@angular/material": "~9.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#55ce98f1db5a3af8c7be0d13e52f85fecb4fc410",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#f4527c93476719e63fe5de576addf0beabb17774",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^14.0.0",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.6",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#df4c57a16e37995b1b1656e41c4821f4f6f326d1",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "github:angular/cli-builds#b425285a091eabfafa79943ba9b49610e04e783e",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#d67f9ac00f16f3289d3a883b66477ce21baa3774",
    "@angular/elements": "github:angular/elements-builds#322c7c939ffed5cc550d2038dc07449cef50d90f",
    "@angular/language-service": "github:angular/language-service-builds#0a1244c6baada62717f499eee903bce3d10944bd",
    "@angular/router": "github:angular/router-builds#b9796454f069651f64a089c2347e76db8ca8676b",
    "@types/chrome": "^0.0.104",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^5.2.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.2",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.8.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.20.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1",
    "webpack": "^4.42.1"
  }
}
