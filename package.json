{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#b2dbc9be7836e6c97a2ff80324cc73c5d0bbb6d5",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "github:angular/common-builds#51a3ef59ae5fc505e2cfd350e4fa21284fa4c6d9",
    "@angular/compiler": "github:angular/compiler-builds#8c42e6bad65587b9f1a61416330d1c898a3b2657",
    "@angular/core": "github:angular/core-builds#009dbd9ec790975dfd14f23649f92b1cc168883c",
    "@angular/forms": "github:angular/forms-builds#85fb93031b316d514cfa85101f5637728e43c371",
    "@angular/material": "~9.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#5a5b58d25994b5103efb4f1782fedf17c9c95b1b",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#e454479f3acf975b8eb2c7c6634ce44a370d4043",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^14.0.0",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.6",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#df4c57a16e37995b1b1656e41c4821f4f6f326d1",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "github:angular/cli-builds#b425285a091eabfafa79943ba9b49610e04e783e",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#bf6af42af664899e1fcc657e09b2a13250587575",
    "@angular/elements": "github:angular/elements-builds#48ba50895ee8caafe9ac52c24a8416cce80b29d9",
    "@angular/language-service": "github:angular/language-service-builds#6368b4c100d5de7476287ada0548bdc15dbd0c8f",
    "@angular/router": "github:angular/router-builds#62cd49c4752df067e7381feafc44fc0ef4bceafc",
    "@types/chrome": "^0.0.104",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^5.2.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.2",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.8.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.20.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1",
    "webpack": "^4.42.1"
  }
}
