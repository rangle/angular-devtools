{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#24b1e486c4416040d5c7b24ce1dae597f3b272b5",
    "@angular/cdk": "~10.2.0",
    "@angular/common": "github:angular/common-builds#fe6fb403f6f673fefa72ee065137978fa2938171",
    "@angular/compiler": "github:angular/compiler-builds#5ac5afaa3602082502fcb2657abd951948e584ed",
    "@angular/core": "github:angular/core-builds#5f1ce2c57cdba7050c9ad510774cbbff5cc54014",
    "@angular/forms": "github:angular/forms-builds#20fa7a1857231696283fd10476257c6c3d5d93fc",
    "@angular/material": "~10.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#19c9592123f1619b39dab66ab29120db19105cd8",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#df5d012bd83789441fe8e0582a9ecd9e38bdca99",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#d882c6f74d4fb654bca60ecd24c01dd11ee2d54b",
    "@angular-devkit/build-ng-packagr": "~0.1001.0",
    "@angular/cli": "github:angular/cli-builds#1b3e5629aa1c98a1184c31b5ee7d2c5a8f573f5c",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#fb654b5c9ffef9c9e709b3e4f750a7b80fd4f6f5",
    "@angular/elements": "github:angular/elements-builds#3cf451a68672fca50ff4e7ffb342961b584f6dbd",
    "@angular/language-service": "github:angular/language-service-builds#a5b64897b0a7e7c8fd3031e394c5628c9b561ec4",
    "@angular/router": "github:angular/router-builds#1f752c7330e0528bd66df082154d3b2d91279231",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.124",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^10.0.0",
    "ngx-build-plus": "^10.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.0.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~3.9.2",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
