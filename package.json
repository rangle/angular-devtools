{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run:ci": "$(npm bin)/cypress run",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shell": "ng test shell-dev --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shell && npm run test:shared-utils",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#a34a5d9f0fc73e1a5ff5ab101261cfeb68ebcfce",
    "@angular/compiler": "github:angular/compiler-builds#555a0aec25b87ce7077b4535f7657bc236d3498d",
    "@angular/core": "github:angular/core-builds#af6c38a2143d05bbb82f6d8d528fa4ed548c5d4e",
    "@angular/forms": "github:angular/forms-builds#c534b9ee75ca0dd0adb09ad0b33ee8146ef44ce3",
    "@angular/animations": "github:angular/animations-builds#a45127c282606ff51c283e90f9c0bda6e9c104bf",
    "@angular/platform-browser": "github:angular/platform-browser-builds#b0b0e764b73f1639cb8c682901806f7aad1b7684",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#555458f4bc085d815e73bcebbbe6e0b65b17a663",
    "@angular/cdk": "~9.0.0",
    "@angular/material": "~9.0.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "ngx-flamegraph": "0.0.3",
    "ngx-vis": "3.0.3",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "scroll-into-view-if-needed": "^2.2.22",
    "tslib": "^1.10.0",
    "uuid": "^3.4.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/router": "github:angular/router-builds#d3759e2d2daa55545a96130e31b3f9ef13c07ef6",
    "@angular/elements": "github:angular/elements-builds#2a2da0ec8b312b506ab7acb415330568ef9859ea",
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#4db2cd8860e97e8ec8adc1610a3f618028d8d1fd",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "github:angular/cli-builds#202b961b10a3ad02044ea0dc10dcbed2440957f3",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#2c06751d7e543d6c139d9e7e8b0624445e49f8f1",
    "@angular/language-service": "github:angular/language-service-builds#e045bbdcb0552e5895e865a732ef5ad65f34ffeb",
    "@types/chrome": "^0.0.91",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^3.8.3",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^1.19.1",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.3.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.0"
  }
}
