{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#8e373742e5d388209e34b4a30cdf7ddf13db81c3",
    "@angular/compiler": "github:angular/compiler-builds#ccf3ae627a0be741b7a16b0f57c53cd8912d8df1",
    "@angular/core": "github:angular/core-builds#cfe115b8bb8c178048f7b9009926f65ab14731ac",
    "@angular/forms": "github:angular/forms-builds#a3200066f5bb5828e92d28650dace6340ac44b5c",
    "@angular/animations": "github:angular/animations-builds#2ff4a1d15d0f51fc1e4bd566a01bf8a11609d9ff",
    "@angular/platform-browser": "github:angular/platform-browser-builds#f60a8984606dc5d2133407abcc8bfbd2525185c9",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#b2e6b9c97005f933a3bf65dd02d5834b8f6a818c",
    "@angular/cdk": "~9.2.0",
    "@angular/material": "~9.2.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^13.0.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "ngx-flamegraph": "0.0.3",
    "ngx-vis": "3.0.3",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "scroll-into-view-if-needed": "^2.2.22",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/router": "github:angular/router-builds#8e792aedce40cd9ac7b5bf732fb235533443abdc",
    "@angular/elements": "github:angular/elements-builds#cfe3a4d151067c431090f18783ecf9c881a20b94",
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#4032a546bac2471243e6fa61c2e5a895e3a8f232",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "github:angular/cli-builds#8b09ac76f8989c8b4f3e0bc61c2278c0af25599d",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#3a010ca94f946fa541a802a1b1479b6ec4cc1a76",
    "@angular/language-service": "github:angular/language-service-builds#826613984bc53db3fbb100bf1eb1e47af5963fd1",
    "@types/chrome": "^0.0.102",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.8.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1"
  }
}
