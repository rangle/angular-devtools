{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "start-no-animations": "yarn start --configuration=e2e",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start-no-animations http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start-no-animations http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#3634d08d244e66b4c45828550e3c8a4010814840",
    "@angular/cdk": "~12.1.0",
    "@angular/common": "github:angular/common-builds#538a10d9a92197917149ca17d31cb25a26ff74fd",
    "@angular/compiler": "github:angular/compiler-builds#06ae7b69d22fa3b48636d6456977ef86036f4b7f",
    "@angular/core": "github:angular/core-builds#0e89f9297b98c54cb33547d2fac0aef963b7c961",
    "@angular/forms": "github:angular/forms-builds#e0d4f09adc1369bb589d3d8ba68ad877914bcc1f",
    "@angular/material": "~12.1.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#46991a3a8f9039d4f6b9c1b6c7de86ab201c7704",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#7fa20898c080bd8380636d0e5e79f84281bdf63b",
    "d3": "^5.7.0",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.10",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#f40f67de45c669697093f7df6bfcb6de6b3aa305",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#3b1faaece957ec0482cf14617732de238ce35ab8",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#9bb6e2ccb308ac011bfa2fa726cdc89f717b78e5",
    "@angular/elements": "github:angular/elements-builds#65b5c4562aee34fd97e5e1e6e33823adc8b90c01",
    "@angular/language-service": "github:angular/language-service-builds#fdabe8a095a6e8e2f5ecbf6889a89210dcce7231",
    "@angular/router": "github:angular/router-builds#d3367a39aba4233c9bcf7b17e4cafe4f845db757",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.145",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.7.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^7.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^12.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~10.1.0",
    "tsickle": "^0.43.0",
    "tslint": "~5.20.0",
    "typescript": "~4.2.3",
    "wait-on": "^6.0.0",
    "webpack": "5.41.1"
  }
}
