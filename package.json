{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#34513862542c27feb5f39fb380e6e9a5bfc9e7e6",
    "@angular/compiler": "github:angular/compiler-builds#1c8464010761c3ef8471219868f0df885d0dbdab",
    "@angular/core": "github:angular/core-builds#1280d42c4e394a6a5c3d3e9d0887c7243dc8918a",
    "@angular/forms": "github:angular/forms-builds#5d9091dac75815e4c0cb734f804b8ad6ebf53f55",
    "@angular/animations": "github:angular/animations-builds#12cc5917fcb0ee0be449a268dc045454e18e7020",
    "@angular/platform-browser": "github:angular/platform-browser-builds#a2d310429bd8aa4ed82b585206dda589194d7f47",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#9a94005b1c1186d6005267d22c2695efbd7ef633",
    "@angular/cdk": "~9.1.0",
    "@angular/material": "~9.1.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "ngx-flamegraph": "0.0.3",
    "ngx-vis": "3.0.3",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "scroll-into-view-if-needed": "^2.2.22",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/router": "github:angular/router-builds#1f6120639ef29ca5b60695a7886de294791f322c",
    "@angular/elements": "github:angular/elements-builds#acb2971f6ba38c2c5e91a92fc7519f69d35aef36",
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#1404f53453c9557e20605783c01515f895e50f25",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "github:angular/cli-builds#490fb9242c2e8d7b4036a98f09f4d177a8a0da78",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#dfef1821376dbf0abee740df2a60314b55eb0a8d",
    "@angular/language-service": "github:angular/language-service-builds#07f2b1c98a4d5ae2b5d32e4ec0c1fc5b10bb6971",
    "@types/chrome": "^0.0.100",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^1.19.1",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.6.0",
