{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#9a9c86ddce5a40c7f1cf8c36c7037053dcb9b776",
    "@angular/cdk": "~11.0.0",
    "@angular/common": "github:angular/common-builds#bb5ff6140ab7cb2e6744fa8c5397a858d85f75d5",
    "@angular/compiler": "github:angular/compiler-builds#6cc49d4b5ca263ca4b03062207b76fb54f16c664",
    "@angular/core": "github:angular/core-builds#4130c88a5d3ca0781fd474b279468cb23d632ebf",
    "@angular/forms": "github:angular/forms-builds#70dd1f71baa6dfdd256e25bba13951e3e9eaf63b",
    "@angular/material": "~11.0.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#252ab177cfe6d96b2e2cdcd79db826a13840bfec",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#144697ddd57684f1a434c90e2855d8f7c05c9267",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#f7344a9d285a243eb16535e0a36e2f2483003521",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#87cef3b31d349ce1b6d5832bd48b05ba016b369a",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#c6074786d8c0adea6a9bbab09b16d1587cee59e0",
    "@angular/elements": "github:angular/elements-builds#f755829471f83435c55248a9de34e3c568cec1c1",
    "@angular/language-service": "github:angular/language-service-builds#a490218276116ff30a95f1e6e1878bc5c7851f31",
    "@angular/router": "github:angular/router-builds#5d55d307f56c1b638be24c6c839a3d0dfae3206c",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.126",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^6.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^11.0.0",
    "ngx-build-plus": "^10.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.0.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~4.0.0",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
