{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#1209d7404e2b238f06d81d7cf5e5f6c85eb4e517",
    "@angular/compiler": "github:angular/compiler-builds#66247be4effbb3dfb10e40947eae883ed20cb662",
    "@angular/core": "github:angular/core-builds#3b775e2c58eb1f645ea4e2f0827e75d20076b3aa",
    "@angular/forms": "github:angular/forms-builds#6448872af59b725762dc52d606bcd2e07b8852b2",
    "@angular/animations": "github:angular/animations-builds#a854a5b8c5e8950c3583df8e8ad080c4977bfba3",
    "@angular/platform-browser": "github:angular/platform-browser-builds#5a274ddd6caa12e48eac18c588bf59632c42e5b9",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#7832d7baa2041d473c6a2491ad0fd71e28692a46",
    "@angular/cdk": "~9.1.0",
    "@angular/material": "~9.1.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^13.0.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "ngx-flamegraph": "0.0.3",
    "ngx-vis": "3.0.3",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "scroll-into-view-if-needed": "^2.2.22",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/router": "github:angular/router-builds#8457b524cd4f24342df615f74243bd3ff1cdf019",
    "@angular/elements": "github:angular/elements-builds#74b54a132aaa039d94bd4831bd058c5fb7be7d3c",
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#373eccb3d31d7145ef28031e96bac205627d3f9e",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "github:angular/cli-builds#f5dd5b16afea218a3f6263fd5c27a8f985da60ed",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#b75058cbcbd766b4a7bb0fa7a30dd4355cf148f8",
    "@angular/language-service": "github:angular/language-service-builds#654ece34ead1a892e933206e1ee3365c8ce98de4",
    "@types/chrome": "^0.0.101",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^1.19.1",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.8.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1"
  }
}
