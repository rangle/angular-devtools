{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#53339d5eaa2b0f775190f9a9ee59eaa6aaa2a49e",
    "@angular/cdk": "~11.0.0",
    "@angular/common": "github:angular/common-builds#79472a791da109e26ad44df6668127f2dbd96715",
    "@angular/compiler": "github:angular/compiler-builds#444400954e84c9cefdf4cc912c15bf5a0d33442b",
    "@angular/core": "github:angular/core-builds#b1cd83914ea9f5e8c6ba69ef5b1224c6988429db",
    "@angular/forms": "github:angular/forms-builds#de61a278749986f5f34b3a0e5505d3aaff3f24a8",
    "@angular/material": "~11.0.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#324ee02bcecb37a4f1978f5b51b4cefaccdc145b",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#eee4a65bd5ef4d0fcb72afb24f08a311bdfd0118",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#5c4d7ceae2bd279dd4bb0ed76c4cff03d6c611d2",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#e1bcfba66e0723b082c1e4e9f81615fef17ddd72",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#900e1b93eb4f6e54ea2f48269cd95cdb17fb6a2f",
    "@angular/elements": "github:angular/elements-builds#83b20ff7256d017e93f18b7e62595c8f973280a3",
    "@angular/language-service": "github:angular/language-service-builds#b90a9c548e9ff7da803fbe7393a94f269bb6c1bc",
    "@angular/router": "github:angular/router-builds#50ddb7f083fdaab18986e2ee0c4180a2a6a6cb20",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.127",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^6.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^11.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.1.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~4.0.0",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
