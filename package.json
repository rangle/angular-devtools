{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#624af950c9ca9c706034997bbb1b288f5c7010d8",
    "@angular/cdk": "~10.1.0",
    "@angular/common": "github:angular/common-builds#66a3ff7f17a1d8cabbaa5e6cf8b3f776ee788ed7",
    "@angular/compiler": "github:angular/compiler-builds#d1c487070c67ec894328d0de67dbf0e59a7901d2",
    "@angular/core": "github:angular/core-builds#3dacd10cdbb1a77f0e4b78b9a82eee18ac452617",
    "@angular/forms": "github:angular/forms-builds#039619b78b3f787b9e9384a8e795fd1675e947c4",
    "@angular/material": "~10.1.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#acd5bee0a1fc29a3b3feb572e3bd6f341168df31",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#c828f9d2c56cdfe2a3b1976510ec73e7a2020d58",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#3155d309842e20b53bb60ed31e2507616b654bd9",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular/cli": "github:angular/cli-builds#d2f2a1a562bbacd94403767990c26a65b4ce7a9c",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#52f89f08700f4575df486dc62be7a29323588258",
    "@angular/elements": "github:angular/elements-builds#cb7b76eacf75d6bbf0c8c4e57421f61fec088ac6",
    "@angular/language-service": "github:angular/language-service-builds#487dd6b7ce06b48563e027de51267a74a5bc659a",
    "@angular/router": "github:angular/router-builds#56e0f8ebc00a9f5e3a98d4857f1efd498f19fb1a",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.122",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^10.0.0",
    "ngx-build-plus": "^10.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.10.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~3.9.2",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
