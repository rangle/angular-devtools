{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#548db3cdbcbd380bde8b5cd177370e24bbe1676b",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "github:angular/common-builds#58a242aad2c75f0dda71c42511696e445ac86daf",
    "@angular/compiler": "github:angular/compiler-builds#a0feb71055d9844024041ebc2b82b1f73485624f",
    "@angular/core": "github:angular/core-builds#26d2d2668650c8c3f86888c8a6da366d0b00d8ca",
    "@angular/forms": "github:angular/forms-builds#33fe64a138437b9340e338e192a79fba5f78dad2",
    "@angular/material": "~9.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#e18cb412fca8a91355031b56232d3f3e34dc5302",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#339fee0659515c2be7ba700b5ee914ca6583d886",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^14.0.0",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.7",
    "rxjs": "~6.5.3",
    "semver-dsl": "^1.0.1",
    "tslib": "^1.10.0",
    "uuid": "^8.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#636e42568df230c125b9ad4b303ed32ed5b54ec3",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "github:angular/cli-builds#f20fd12182eadd2abff52502592fbd06ceb9bd3d",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#71a7c870f27ac1b5d64ae0f22a1418f7d9d3e35a",
    "@angular/elements": "github:angular/elements-builds#3742f71d52ddf3025fdd19407f86e4a21bff0d3e",
    "@angular/language-service": "github:angular/language-service-builds#5c644f932b1ae795038e4b775fc1fdd791e9d04e",
    "@angular/router": "github:angular/router-builds#217bfce85f71c5a015cd1ead19964742ab271bfa",
    "@types/chrome": "^0.0.107",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^5.2.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.2",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.10.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.20.0",
    "typescript": "~3.8.3",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
