{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#25d249828650337d602f05c947f6d55c9b392463",
    "@angular/cdk": "~10.2.0",
    "@angular/common": "github:angular/common-builds#7d9b4553749d1a8979be64f5b747eba43f02a85b",
    "@angular/compiler": "github:angular/compiler-builds#6dc4e5fa831749d2d60e9720f583d97861289e99",
    "@angular/core": "github:angular/core-builds#4ffa28a6777ffe01c0ca75361fda7e0bd0e5f51b",
    "@angular/forms": "github:angular/forms-builds#2a5c8f65666fe9a9a365640ab5c35add6fd3d0af",
    "@angular/material": "~10.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#cd8b215b901454f71b8d8af326c6cb79a4fcda3b",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#b6cffd836d1cf6b3d1dd52ac5e6e6757efd23bf5",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#bf75bef3322d3162eea097f39b32d88175768637",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular/cli": "github:angular/cli-builds#aa2608b3ba30b4fd10c197ad5d4c88502d08cab0",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#4fe51921015c4f40e6b9a28aa7e02c3c4a246964",
    "@angular/elements": "github:angular/elements-builds#210499ca516e0f78c2759b469f9d82d5689969ce",
    "@angular/language-service": "github:angular/language-service-builds#3b949f88bef5cd75ea593dec3a5ebf48b5697ba9",
    "@angular/router": "github:angular/router-builds#390bf501a96f4caacdcab05aa762f8aceccdc57c",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.123",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^10.0.0",
    "ngx-build-plus": "^10.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.0.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~3.9.2",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
