{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#35f2519079ba49a68295c21fdfefe0ad74763521",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "github:angular/common-builds#180e7a91ce10bedd77824e942ee9dd0a2e81b55c",
    "@angular/compiler": "github:angular/compiler-builds#e34e9e9dacbae7a84fb128296e950e4aed42849f",
    "@angular/core": "github:angular/core-builds#9287d612fc6f6630560c8027b36b1007f7b43231",
    "@angular/forms": "github:angular/forms-builds#7d74c328c9556ff96859bba4d15d3d673e2fa79d",
    "@angular/material": "~9.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#ff65ebdae0723e50961310428d039d2fdc004584",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#302c9718dea793337d0fcc048242be267ced87a8",
    "@swimlane/ngx-charts": "^14.0.0",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.7",
    "rxjs": "~6.5.3",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "uuid": "^8.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#012a3a53b4fe96e261a6f7d887251b1de553829c",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "github:angular/cli-builds#5a13fe8177c900e3aa5bf39f03a0a2e5acc24149",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#e8ab937704c5a53721f948bd77a2c52896f75096",
    "@angular/elements": "github:angular/elements-builds#57e3f7176052ee6f49b90c64131d373abd3e9eaa",
    "@angular/language-service": "github:angular/language-service-builds#ada8fc1b638cba2872675ac49bb95f59ab735677",
    "@angular/router": "github:angular/router-builds#f54fb42db5f5e6f50f9b70b03780c361579ef0e0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.112",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^5.2.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.10.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.20.0",
    "typescript": "~3.8.3",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
