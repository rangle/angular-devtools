{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#345024f1a666546ce883a659e071d962bb88d2e4",
    "@angular/cdk": "~10.2.0",
    "@angular/common": "github:angular/common-builds#0beca8b444f7a022da7e6b40d12b3db95d494ff0",
    "@angular/compiler": "github:angular/compiler-builds#18be36de8664960cf16c72472c3e226c28b79f73",
    "@angular/core": "github:angular/core-builds#300f7681a1573bd2ff56a709d70124af7e3a4505",
    "@angular/forms": "github:angular/forms-builds#675145a1168278d4450d3b465ffe0ba680bc7c4c",
    "@angular/material": "~10.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#82f9d7e513c579309519375b78d6fbd17152527f",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#c9b940d41573aa73afd9ac70cbfd68926090ad63",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#9aaa70a777b2f5148451b1695f600622bc530e00",
    "@angular-devkit/build-ng-packagr": "~0.1001.0",
    "@angular/cli": "github:angular/cli-builds#8416fd10c5d0a1c4dafc07f66335dd36bd77466c",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#f3014f3fa763208216513e2eb5d741b9349cfdfb",
    "@angular/elements": "github:angular/elements-builds#12fcad5c88a0dfa1ed07279d9b2893a94ce99201",
    "@angular/language-service": "github:angular/language-service-builds#875187f2dcdf9be2c2ce74525f2e06c8bd155538",
    "@angular/router": "github:angular/router-builds#44a55eefd30822dad8f2fbbbb09cefd8db93fece",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.123",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^10.0.0",
    "ngx-build-plus": "^10.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.0.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~3.9.2",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
