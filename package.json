{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "start-no-animations": "yarn start --configuration=e2e",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start-no-animations http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start-no-animations http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#b372e0c10bdb4e6226edfa5165fda6a559c43578",
    "@angular/cdk": "~12.1.0",
    "@angular/common": "github:angular/common-builds#12fdb44178065b13c16e5ebb84d1933ea7af4fe6",
    "@angular/compiler": "github:angular/compiler-builds#40d2eb99cbd4695776c37689d2392f3c4b2144de",
    "@angular/core": "github:angular/core-builds#a56032344dc27b671af1f84da823738c1f8cf081",
    "@angular/forms": "github:angular/forms-builds#2ef7dd4a0f2068efe28d87bfef57b985476bc572",
    "@angular/material": "~12.1.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#8de64b67eb15d5294392817f2e3bd741816a62bd",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#cb509b3797c5ec2a19778bfb1d169779c174993b",
    "d3": "^5.7.0",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.10",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#3e34f45fb9024bc709ed113c37e0885d1bdb2fae",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#00a931d91b2b41a1ddedda6da22971f9208e0093",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#a360e3608c86d40d4fffca14a432100c825f3a30",
    "@angular/elements": "github:angular/elements-builds#5d18b924c9220db23faf2678491c2b1efd2725f8",
    "@angular/language-service": "github:angular/language-service-builds#81397775bc8dbf4c6ecd5c8ac2746ecdc8b6cc9e",
    "@angular/router": "github:angular/router-builds#59fc5bb298cd616c7d05d4e5f244c567a169cd5e",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.149",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.7.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^8.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^12.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~10.2.0",
    "tsickle": "^0.43.0",
    "tslint": "~5.20.0",
    "typescript": "~4.2.3",
    "wait-on": "^6.0.0",
    "webpack": "5.46.0"
  }
}
