{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#a471659814ce93885b2ca2693d52dbf2030d981d",
    "@angular/cdk": "~10.1.0",
    "@angular/common": "github:angular/common-builds#2d5bba2f8e457a98fb891adf70b1910e5ee4e853",
    "@angular/compiler": "github:angular/compiler-builds#24ed628aff1bb0f0918a6bf0dd49b8ae48ba455f",
    "@angular/core": "github:angular/core-builds#a09aa08731347bc680ccf5e4288298b3ff155ad2",
    "@angular/forms": "github:angular/forms-builds#7c20cebd4ea11db9c1b1a4e8cdc7da03012fd1e4",
    "@angular/material": "~10.1.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#46ab259e382726f0315e996bb94523602526cf78",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#c87bba4bf1e0c564dbee8a92daf273bb3e195d89",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#953c65819fc7144f1ce1b02d9c6910bb0f846453",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular/cli": "github:angular/cli-builds#42d536a507b58224e1b18e1ffc3afecde4763b78",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#d6105c409bdf3222bc5f48e4d30a40083159f79f",
    "@angular/elements": "github:angular/elements-builds#c38d6a6c07745cf6dd3e1e09e064ea652c91a166",
    "@angular/language-service": "github:angular/language-service-builds#c920a4409bc5e4114dfb87b64dd1bfd849e50347",
    "@angular/router": "github:angular/router-builds#facdea7bd4fe80eadf6ade1c2664841f30b4395a",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.122",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^10.0.0",
    "ngx-build-plus": "^10.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.0.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~3.9.2",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
