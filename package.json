{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#241114f6a5f25b6de039cd0f8bdec3b81293cd26",
    "@angular/cdk": "~11.0.0",
    "@angular/common": "github:angular/common-builds#d33745af415a5c8af78210e6280eba163e950315",
    "@angular/compiler": "github:angular/compiler-builds#315fa70cdb9d9cf240423e29decd1427c3f56766",
    "@angular/core": "github:angular/core-builds#858f91b94136e11480dbb4720ddb74cf0e906bcb",
    "@angular/forms": "github:angular/forms-builds#7f441872844d4b45874ae782cc7946f6c9b4802d",
    "@angular/material": "~11.0.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#af320d727df072d63c2c5ff1fa2ba12aa7bef99e",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#2a615acf037d3cc5ad3f9da4fa3d34e106b1592a",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#0c3a638c8caee4f080a0e842f9b016e9a20f5f10",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#f6f0cde414b8d7c8fac022efe0d01f6e926e8359",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#706be339202b2a38002668621efed783a2bd6143",
    "@angular/elements": "github:angular/elements-builds#487928cd4fe53b17ac8f5476f7d099c6f0284e80",
    "@angular/language-service": "github:angular/language-service-builds#c919dbe0d0541eb93c8327a45f8c82e3a84fb14c",
    "@angular/router": "github:angular/router-builds#890ce33c63a7eed9750a0c947f35cef6a8e6ee97",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.126",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^6.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^11.0.0",
    "ngx-build-plus": "^10.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.1.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~4.0.0",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
