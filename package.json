{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#cb014a84388d1e324dbc53bdbd355f1b575a8553",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "github:angular/common-builds#e806d15c24ce01bd67f80d5f7259375582d5f703",
    "@angular/compiler": "github:angular/compiler-builds#659b1ad73aaeb6635a8db83fa6cba399ac39943a",
    "@angular/core": "github:angular/core-builds#1f6a2942622f6aa2625b18cbc2661395505d3536",
    "@angular/forms": "github:angular/forms-builds#b28beec0915d1bda6c5d1ff2b32fc0765b00efab",
    "@angular/material": "~9.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#095c98f96c4f19fa6bf7cdf0e38728edbf35a5ce",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#c9f1857ea3c8fde71f845e4a2ca43372fb88fdd9",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^14.0.0",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.7",
    "rxjs": "~6.5.3",
    "semver-dsl": "^1.0.1",
    "tslib": "^1.10.0",
    "uuid": "^8.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#7e15658eb257ba18dd6e46e6d360ddc5eb771704",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "github:angular/cli-builds#3d56d7d78ca5aed4879b46c6abd572adf615c1e1",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#df66a5518ce1eba23da2f55b34a11a5a4c4a6180",
    "@angular/elements": "github:angular/elements-builds#6a4cf845ba497872337926d914c3f9cde89af3f7",
    "@angular/language-service": "github:angular/language-service-builds#273ff81efe51a98970a249dab2f2ea79bff9cccb",
    "@angular/router": "github:angular/router-builds#8b084f6377e91a9d4d07411faaef0f4685d312b5",
    "@types/chrome": "^0.0.107",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^5.2.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.2",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.10.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.20.0",
    "typescript": "~3.8.3",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
