{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#6314c3ea23fb768969bf15a4adc476ba2d1820d0",
    "@angular/compiler": "github:angular/compiler-builds#9cfaf18add13edb805b61f871b02b5a013363ab6",
    "@angular/core": "github:angular/core-builds#8d254c6164732c0c068e29f8c4360ae307ac0d8d",
    "@angular/forms": "github:angular/forms-builds#786c6b48743d7ad51e3080a6f363b0d428605b50",
    "@angular/animations": "github:angular/animations-builds#4da4358c04998a53b200402e2b013989704b93e5",
    "@angular/platform-browser": "github:angular/platform-browser-builds#dda287fcd9f915d308a0f2bdaa09024a46aae220",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#f52b4b3aac9030eca3c496620ff487428107a7bb",
    "@angular/cdk": "~9.2.0",
    "@angular/material": "~9.2.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^13.0.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "ngx-flamegraph": "0.0.6",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/router": "github:angular/router-builds#372b68c5fdb6c50bba5bf246fd0556e66018a4f2",
    "@angular/elements": "github:angular/elements-builds#06a11888654b3f673c50e853bc779e1f2199081e",
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#15856cc5410cb9bc9565326a8a6e2dbdb325e89b",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "github:angular/cli-builds#8f50afd05a8d4003d50588ad9be3a1d580ed2290",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#eccfc0b2f430963f5a50b5cb4d42d10bc8c698b8",
    "@angular/language-service": "github:angular/language-service-builds#30470aafa1ba03edfc2db92ff60c9967fa7a34cf",
    "@types/chrome": "^0.0.103",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.8.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1"
  }
}
