{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#6d6b99b5905861fb1dec8c9c31b01568c16e87ca",
    "@angular/compiler": "github:angular/compiler-builds#1eb221915a039c37c69f029412d1e0c000ed377d",
    "@angular/core": "github:angular/core-builds#88f96615f445259a04e9168f9d85fcace4b0370b",
    "@angular/forms": "github:angular/forms-builds#632577797fb3a959d318d5c39140ad749833bf09",
    "@angular/animations": "github:angular/animations-builds#af07b679d839a540560a9c7325192ad722360252",
    "@angular/platform-browser": "github:angular/platform-browser-builds#efcb775e14aba3dd6c4a8f8379ad79cc27d10a8a",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#a8b330aefc508a334b555e6cceb53876a90d3018",
    "@angular/cdk": "~9.1.0",
    "@angular/material": "~9.1.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^13.0.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "ngx-flamegraph": "0.0.3",
    "ngx-vis": "3.0.3",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "scroll-into-view-if-needed": "^2.2.22",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/router": "github:angular/router-builds#cf9462e92da507b497aa0fa344f46f6e5ef076d0",
    "@angular/elements": "github:angular/elements-builds#60ed29418e2fccc296eb9682a10e12cf26176240",
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#373eccb3d31d7145ef28031e96bac205627d3f9e",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "github:angular/cli-builds#f5dd5b16afea218a3f6263fd5c27a8f985da60ed",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#5a75cbfefc2d4b23c7622a5308b5ac214678db62",
    "@angular/language-service": "github:angular/language-service-builds#43ee14a4087baa173a7a5aefdcf99201a94ce26e",
    "@types/chrome": "^0.0.101",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.8.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1"
  }
}
