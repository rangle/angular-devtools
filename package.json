{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#90ea161910d238e6a668ef374127842dd3680a13",
    "@angular/compiler": "github:angular/compiler-builds#786fd81df51bce07f84048e6aacf2e5925a38eb5",
    "@angular/core": "github:angular/core-builds#6685b345348efe77d890b7fc082265f98355b969",
    "@angular/forms": "github:angular/forms-builds#2da8dcbb8806bfdd9309cfd250ad28dd5de4d811",
    "@angular/animations": "github:angular/animations-builds#893d7c6c2a6fe6970892bf4d95028c231c1a72d8",
    "@angular/platform-browser": "github:angular/platform-browser-builds#698aba9a45049bf327a6b0c4c52456bd3ff4f88a",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#e0405a5ad94dc9d16f186ae9d5d2c665569e73e8",
    "@angular/cdk": "~9.1.0",
    "@angular/material": "~9.1.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^13.0.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "ngx-flamegraph": "0.0.3",
    "ngx-vis": "3.0.3",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "scroll-into-view-if-needed": "^2.2.22",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/router": "github:angular/router-builds#fb1cff279e084b901589f05ffc85d44675d0b0f2",
    "@angular/elements": "github:angular/elements-builds#b5884f46d4ce8f841418d175eb93f1ed46d892a4",
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#1404f53453c9557e20605783c01515f895e50f25",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "github:angular/cli-builds#490fb9242c2e8d7b4036a98f09f4d177a8a0da78",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#7840f8a3a41b659eaf41557947943a9c273655fd",
    "@angular/language-service": "github:angular/language-service-builds#d7e8b6d02db4c159de1c5746092b2edc225c0019",
    "@types/chrome": "^0.0.100",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^1.19.1",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.6.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1"
  }
}
