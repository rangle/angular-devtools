{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "start-no-animations": "yarn start --configuration=e2e",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --configuration production",
    "test": "ng test",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "e2e": "npm run cy:run",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start-no-animations http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start-no-animations http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#71f8be3cbfc096615fa53a2efb1e76a0e804161b",
    "@angular/cdk": "~12.2.0",
    "@angular/common": "github:angular/common-builds#1ad14942e69e2340e76fb495b4557e42f04a1952",
    "@angular/compiler": "github:angular/compiler-builds#2155540d6d86f09e8c5cc1bf788ad60288d3eeb0",
    "@angular/core": "github:angular/core-builds#15db5be7501217a0893b23f47a2a47f80c4d3401",
    "@angular/forms": "github:angular/forms-builds#f9f99be58ea32c9e634d8b91db2c1a42ca5b7ebd",
    "@angular/material": "~12.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#4a8e68a0e834bcbfca36c7324823e7d2912feced",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#7fef54602301196f3abcd505e524869e7f0c93de",
    "d3": "^5.7.0",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.10",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#9086b8266e7fb722ab18495b812fae6d74d68079",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#3e1325c7345af80d307c54676f87f73efc0a1b3b",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#c50f0c6cf0c45d8d5d6b5f7ae0593046aa576e2b",
    "@angular/elements": "github:angular/elements-builds#bc1a36ed7bbc90d3acfaa2bcc11b5655f59972d8",
    "@angular/language-service": "github:angular/language-service-builds#8506871de16cfdb3a89f5c632f33597396783239",
    "@angular/router": "github:angular/router-builds#5b710a02dc79dcb475d78fc67ab502b8bb5788af",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.157",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.9.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^8.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^12.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~10.2.0",
    "tsickle": "^0.43.0",
    "tslint": "~5.20.0",
    "typescript": "~4.2.3",
    "wait-on": "^6.0.0",
    "webpack": "5.53.0"
  }
}
