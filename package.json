{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "start-no-animations": "yarn start --configuration=e2e",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start-no-animations http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start-no-animations http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#a2e6cd9551675a482c3aaedb979b425a1b96a79b",
    "@angular/cdk": "~12.1.0",
    "@angular/common": "github:angular/common-builds#fe5ca22cb8adc1e4c363ca65174f306b1129c166",
    "@angular/compiler": "github:angular/compiler-builds#554ccde440db2fefcf431c1ce6efd31628d0d30b",
    "@angular/core": "github:angular/core-builds#67a1e6b122f7ba00c652dead7255101acaa66d89",
    "@angular/forms": "github:angular/forms-builds#e0c6e82f33b52539da1b8872f58f6220cf2d3919",
    "@angular/material": "~12.1.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#386f6cf422922b077150627ffa26349afe890884",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#38f82eaf5be108fcfc6038820c9bdd1b2745f09a",
    "d3": "^5.7.0",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.10",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#58452bb1ecb40a662c06bbd005415946a8d62eed",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#f8851b28efb89adc3670653f623b1d93dfa5acc6",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#9a0459315eb4bf8a1763795ae92f93aa55d52c1a",
    "@angular/elements": "github:angular/elements-builds#9950c439c1dc87b1ee81a8687b19764f6f01fd4a",
    "@angular/language-service": "github:angular/language-service-builds#95d99a3b06e3d5e74ee40df9594eb5a6fc0a6745",
    "@angular/router": "github:angular/router-builds#6bacaaa45706d5cbef12a7cf78e6db9bc01b9ea6",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.145",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.7.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^8.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^12.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~10.1.0",
    "tsickle": "^0.43.0",
    "tslint": "~5.20.0",
    "typescript": "~4.2.3",
    "wait-on": "^6.0.0",
    "webpack": "5.41.1"
  }
}
