{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#d7f9ecd1d5e5876a0f76f6076b7dbabbcd09c27d",
    "@angular/cdk": "~11.0.0",
    "@angular/common": "github:angular/common-builds#f26f3e3ce36418a744c1df3bd7db1e802fc6f3ea",
    "@angular/compiler": "github:angular/compiler-builds#6f123f8339676c3e0e3cef527eb0dd04a4b24cf1",
    "@angular/core": "github:angular/core-builds#3f395a622b1b02ca20f8e63b22628df6f50c7ba2",
    "@angular/forms": "github:angular/forms-builds#26740cadcd31d77886b0e07158379e6d9b90903c",
    "@angular/material": "~11.0.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#e569fef6e89f52b86558e97bb2f34d4838155baa",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#d084aa276cdd1b8b6135aaf9337169a8cfeb3733",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#3ddfa78f6cd776e670703257fddf5c8a4f6696c0",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#992a7883669c427f7234f753e526acb67553fa48",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#8c34d4191cd21eb3b8d6ee1be574e241f85365eb",
    "@angular/elements": "github:angular/elements-builds#5fb6fcb95e651088c1e027bb9c6a0607a1a5e3b1",
    "@angular/language-service": "github:angular/language-service-builds#ea58650195aeb88bdd5aa75218c071bd603a8cc6",
    "@angular/router": "github:angular/router-builds#8e9235fa1ddd9d82ec0dd3b84b4389a3992d66ad",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.128",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^6.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^11.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.1.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~4.0.0",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
