{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#beca6795e1d062adb96751312ff0fe2d3d3f8c65",
    "@angular/cdk": "~9.2.0",
    "@angular/common": "github:angular/common-builds#15e2d43b6f0327843dd5a1fd63b441c11a89d15c",
    "@angular/compiler": "github:angular/compiler-builds#4dce467714dfd05fac91d3df38af0f6651e21f6e",
    "@angular/core": "github:angular/core-builds#98447cb0775ff30e5c2e3715e58be83763631ece",
    "@angular/forms": "github:angular/forms-builds#9ecddd713e2c8dd456b4e7af05a9d86f6bc28f98",
    "@angular/material": "~9.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#0fe5a90211a54fbdd1205b717d5ab0214565af08",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#42a929a42054e16f9b56fd64ba85b384732609e6",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^13.0.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.6",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#15856cc5410cb9bc9565326a8a6e2dbdb325e89b",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "github:angular/cli-builds#8f50afd05a8d4003d50588ad9be3a1d580ed2290",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#368b24b78b55abde554ee6a313d3385fcef3d731",
    "@angular/elements": "github:angular/elements-builds#b271fb6e270c036e63633679b7bf64fd849e99c8",
    "@angular/language-service": "github:angular/language-service-builds#c640e37efc6a229be42059795778859e58ce9d23",
    "@angular/router": "github:angular/router-builds#4b9e484d9569d00a1f87bf65d609d938f21828e0",
    "@types/chrome": "^0.0.103",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^2.0.0",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.8.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1"
  }
}
