{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "start-no-animations": "yarn start --configuration=e2e",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start-no-animations http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start-no-animations http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#5b26a8c205be89e9aba218b76fa9a38360c9595a",
    "@angular/cdk": "~11.2.0",
    "@angular/common": "github:angular/common-builds#4315d6e46ec0958c77549f1c667e33a28c57b74e",
    "@angular/compiler": "github:angular/compiler-builds#62c93316fd9a3e1022d952012600baf192869a71",
    "@angular/core": "github:angular/core-builds#954f5a520456d8ad5a214e400c640487afe8f3d9",
    "@angular/forms": "github:angular/forms-builds#af4c80fd0cd4b97e21772d5aa3fdb6513745f809",
    "@angular/material": "~11.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#da74299dd5f3080b5e72a3658b90f2daab2c5c39",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#95e660e13b5baeee9f7bdbe4c287c709d06ae91f",
    "d3": "^5.7.0",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.10",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#ece77fbe2f5d90d0aa4aca213e678726dfa5d139",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#d00d2bd7b64afbaf7302b696192e543605304c36",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#23eff08643dd07146986f74754b30ccd996d3be1",
    "@angular/elements": "github:angular/elements-builds#2ab7d1095aff1d18a68ca5755188554750b22b13",
    "@angular/language-service": "github:angular/language-service-builds#6196dae2014af947320671240b0977723a2c7463",
    "@angular/router": "github:angular/router-builds#c573df9be6db600ae5e8a1fa79b7d0033080236e",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.137",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.7.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^7.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^11.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.1.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~4.2.3",
    "wait-on": "^5.0.0",
    "webpack": "5.36.1"
  }
}
