{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "start-no-animations": "yarn start --configuration=e2e",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start-no-animations http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start-no-animations http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#b5ab0928a93223b2d02ae1b65e29b44840e648f6",
    "@angular/cdk": "~12.0.0",
    "@angular/common": "github:angular/common-builds#1eecaff697fbd27011d2f33496bacff1b76030a1",
    "@angular/compiler": "github:angular/compiler-builds#4710bd691ad7ce943f548b8ff06c2f0227882e15",
    "@angular/core": "github:angular/core-builds#ee550a1d63230bda6ddf037ce96c35091f69d0db",
    "@angular/forms": "github:angular/forms-builds#aef3cc3e84e113e01eace0d3b8175c6e0fb563ca",
    "@angular/material": "~12.0.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#45280c5c9c5d625433fdcad0964e038a710f1122",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#deedf860b410f2cf5b675ece9f43a27957f8c5b0",
    "d3": "^5.7.0",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.10",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#26eb107100248a2216a16d2df67ad920a72fc9d7",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#e9b9f04307948e0642cf1e8864e3eaac3905ac6c",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#9cb51cde1d0cf29840d1a2f939343eaa7c9ae80d",
    "@angular/elements": "github:angular/elements-builds#edfb86dace6c7c21cf8e4d418a3d2fdc0995bcb5",
    "@angular/language-service": "github:angular/language-service-builds#e70ed49b1005682aa24eee80da0a5d6a20587703",
    "@angular/router": "github:angular/router-builds#29ddc9f676f29828aa984288ba2c08466229f588",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.144",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.7.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^7.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^12.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~10.0.0",
    "tsickle": "^0.40.0",
    "tslint": "~5.20.0",
    "typescript": "~4.2.3",
    "wait-on": "^5.0.0",
    "webpack": "5.38.1"
  }
}
