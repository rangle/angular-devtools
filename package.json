{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress run",
    "cy:run:watch": "ng serve & wait-on http://localhost:4200 && $(npm bin)/cypress open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "github:angular/common-builds#6ce1c5364b22a305e92b48015c403d472f2d678f",
    "@angular/compiler": "github:angular/compiler-builds#bc43a5ba996b43e8b3368c68673818b383b7d0e0",
    "@angular/core": "github:angular/core-builds#e31ba992b6e72c611de7ba29e793c4042fa9e498",
    "@angular/forms": "github:angular/forms-builds#ff55c94d0a3d27751cc3a869d2671a624db09d12",
    "@angular/animations": "github:angular/animations-builds#f8c1d1bdd5f1086623f0489081ebe181abff3155",
    "@angular/platform-browser": "github:angular/platform-browser-builds#cc3d3dbc2e6266450b18bd1736f1da98db4ae9a7",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#4b02c69b6e3cffd6d4b0c1b05cac45b4a31ad063",
    "@angular/cdk": "~9.1.0",
    "@angular/material": "~9.1.0",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@swimlane/ngx-charts": "^13.0.2",
    "angular-split": "^3.0.3",
    "clone-deep": "^4.0.1",
    "document-register-element": "^1.7.2",
    "file-saver": "^2.0.2",
    "ngx-flamegraph": "0.0.3",
    "ngx-vis": "3.0.3",
    "pretty-quick": "^2.0.1",
    "rxjs": "~6.5.3",
    "scroll-into-view-if-needed": "^2.2.22",
    "tslib": "^1.10.0",
    "uuid": "^7.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/router": "github:angular/router-builds#8dd7594180ce8b8869a09630e9232b91c50db062",
    "@angular/elements": "github:angular/elements-builds#10e28e0120b4fb90594e18e633197c1e7f34dad8",
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#1404f53453c9557e20605783c01515f895e50f25",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "github:angular/cli-builds#490fb9242c2e8d7b4036a98f09f4d177a8a0da78",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#efca68105f77e60733dd02d84f0ee2fec60fcd61",
    "@angular/language-service": "github:angular/language-service-builds#edecc0eedf17ed37d50731f2b71bf7e417db4813",
    "@types/chrome": "^0.0.100",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.2.1",
    "cypress": "^4.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "ngx-build-plus": "^9.0.4",
    "prettier": "^1.19.1",
    "protractor": "~5.4.2",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~8.6.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.0",
    "wait-on": "^4.0.1"
  }
}
