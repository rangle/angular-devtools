{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "start-no-animations": "yarn start --configuration=e2e",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --configuration production",
    "test": "ng test",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "e2e": "npm run cy:run",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start-no-animations http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start-no-animations http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#ea0af7ae05da15d160e146d2614ec4e8432345c0",
    "@angular/cdk": "~12.2.0",
    "@angular/common": "github:angular/common-builds#4b2bb4f77e3cd11f0ec0a9f9eb5803864ab47522",
    "@angular/compiler": "github:angular/compiler-builds#352f612c654b1286234972ecd99502d33ed98c95",
    "@angular/core": "github:angular/core-builds#1f7fc9c611712757df3c547a70ffda0dde8ccc9a",
    "@angular/forms": "github:angular/forms-builds#70a20cdab5fd8955c872596549db4a4a05a5571c",
    "@angular/material": "~12.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#dab22eadb7fde875db23e104894c31550c3b19e5",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#c463083acbe8d9de80ba4bdbad60c17ca7a8bd98",
    "d3": "^5.7.0",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.10",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#86bed501c1c530ff0e99f456aee7ab734c29522a",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#a2757bb9dfbdefa9e4598ae82b1d2ae72b742efb",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#8d3427b0019750f528485ded7445f387b2cb909e",
    "@angular/elements": "github:angular/elements-builds#bd210922e16ca51458fb226319b04338f9e06c73",
    "@angular/language-service": "github:angular/language-service-builds#1da1215308f440fc370d4d32e4a5c30620b3ad9b",
    "@angular/router": "github:angular/router-builds#1a19e2a406f78a54680be50d5540820171b93578",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.157",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.9.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^8.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^12.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~10.2.0",
    "tsickle": "^0.43.0",
    "tslint": "~5.20.0",
    "typescript": "~4.2.3",
    "wait-on": "^6.0.0",
    "webpack": "5.53.0"
  }
}
