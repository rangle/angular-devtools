{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#8e2353c0cb79ab17192ea7d3664eba179c592001",
    "@angular/cdk": "~10.1.0",
    "@angular/common": "github:angular/common-builds#c41a4d3a5bb11a5d754aa245bc6bbbfbfda68cb7",
    "@angular/compiler": "github:angular/compiler-builds#159d00098f335b9f613c1c6e136bff1b2f0de962",
    "@angular/core": "github:angular/core-builds#89d97681806ba2535cc1e3fd2126f6d3c1f6a144",
    "@angular/forms": "github:angular/forms-builds#21a2d913f33094f28c9f468dacc3257e5237f028",
    "@angular/material": "~10.1.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#beb42eeb7378c5c703c204ddaa9ec0776bd41068",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#037c3d1412c62cad34bc38a40aee3bf3cd957160",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.8",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#6a8e520d3c24deb60e5f08b885b43bdba62e3144",
    "@angular-devkit/build-ng-packagr": "~0.1000.0",
    "@angular/cli": "github:angular/cli-builds#521fb474e43cafacacb99a4bebc0fdb5291dc8eb",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#5718d734b5d252c619d3d33cddb31c24683dda2f",
    "@angular/elements": "github:angular/elements-builds#6c22ce0ad9fe5d2ab9606d5152c2f342f2784154",
    "@angular/language-service": "github:angular/language-service-builds#21fab950cf31841244df3380af1ab3943f601d58",
    "@angular/router": "github:angular/router-builds#b7238279fd7040ae1b5dd8d1c06549183a739098",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.122",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.2.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^10.0.0",
    "ngx-build-plus": "^10.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.0.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~3.9.2",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
