{
  "name": "angular-devtools",
  "version": "0.0.0",
  "scripts": {
    "postinstall": "ngcc --async false --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "ng",
    "cypress:verify": "cypress verify",
    "start": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng serve",
    "build": "ng build",
    "build:chrome": "cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome",
    "release": "node projects/shell-chrome/set-version.js && cross-env LATEST_SHA=`git rev-parse --short HEAD` ng build shell-chrome --prod",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "npm run cy:run",
    "e2e:watch": "npm run cy:run:watch",
    "cy:verify": "$(npm bin)/cypress verify",
    "cy:version": "$(npm bin)/cypress version",
    "cy:run": "$(yarn bin)/cypress run",
    "cy:open": "$(yarn bin)/cypress open",
    "cy:ci": "start-server-and-test start http-get://localhost:4200 cy:run",
    "cy:watch": "start-server-and-test start http-get://localhost:4200 cy:open",
    "test:backend": "ng test ng-devtools-backend --watch=false",
    "test:devtools": "ng test ng-devtools --watch=false",
    "test:shared-utils": "ng test shared-utils --watch=false",
    "test:protocol": "ng test protocol --watch=false",
    "test:ci": "npm run test:backend && npm run test:devtools && npm run test:shared-utils && npm run test:protocol",
    "prettier": "prettier --write \"{,!(node_modules|dist|build|coverage)/**/}*.{js,jsx,ts,tsx,json}\"",
    "prettier:fix": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier:fix"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "github:angular/animations-builds#953c584126a329e3a10852596d978bf39e02d570",
    "@angular/cdk": "~11.2.0",
    "@angular/common": "github:angular/common-builds#538ee9e30523d5fd7de3fec2b64c7bae6d187911",
    "@angular/compiler": "github:angular/compiler-builds#056c58cb63eec3b42460c0aed881fdd8dc867c13",
    "@angular/core": "github:angular/core-builds#d4bc10cdee0f548536f3f2faf5666cff93fb085e",
    "@angular/forms": "github:angular/forms-builds#fefa1769a9898fb6db658d8a95a490b26712b4ab",
    "@angular/material": "~11.2.0",
    "@angular/platform-browser": "github:angular/platform-browser-builds#0d09bef909ee4928a7c0e7b07c856e778db796ac",
    "@angular/platform-browser-dynamic": "github:angular/platform-browser-dynamic-builds#39cb549331f569f7fb65daca932a12eff79a68a0",
    "d3": "^5.7.0",
    "memo-decorator": "^2.0.1",
    "ngx-flamegraph": "0.0.10",
    "rxjs": "~6.6.0",
    "semver-dsl": "^1.0.1",
    "tslib": "^2.0.0",
    "webtreemap": "^2.0.1",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "github:angular/angular-devkit-build-angular-builds#1eeb2858411d3a75bd42a2386cab19fd2e21c68c",
    "@angular-devkit/build-ng-packagr": "~0.1002.0",
    "@angular/cli": "github:angular/cli-builds#0343303ce51abed35d55db15da5f295cf3f2cb70",
    "@angular/compiler-cli": "github:angular/compiler-cli-builds#f3a4b46f3252122b864f7cecb0a44c817c6fbad5",
    "@angular/elements": "github:angular/elements-builds#442fe83cc8d8ca2d954e44ae922cfe8ca7d90217",
    "@angular/language-service": "github:angular/language-service-builds#ee557411daf69f114eafb975766cb732f9c0fe32",
    "@angular/router": "github:angular/router-builds#748630e04e345b482142b7c33ea22902549fa715",
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@types/chrome": "^0.0.134",
    "@types/clone-deep": "^4.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/semver": "^7.1.0",
    "codelyzer": "^6.0.0",
    "cross-env": "^7.0.2",
    "cypress": "^6.0.0",
    "cypress-browser-extension-plugin": "^0.1.0",
    "cypress-iframe": "^1.0.1",
    "document-register-element": "^1.7.2",
    "husky": "^4.3.8",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^11.0.0",
    "ngx-build-plus": "^11.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.10.11",
    "todomvc-app-css": "^2.3.0",
    "todomvc-common": "^1.0.5",
    "ts-node": "~9.1.0",
    "tsickle": "^0.39.0",
    "tslint": "~5.20.0",
    "typescript": "~4.2.3",
    "wait-on": "^5.0.0",
    "webpack": "^4.42.1"
  }
}
